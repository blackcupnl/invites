{
    "name": "blackcup/invites",
    "description": "Package to create invites",
    "license": "MIT",
    "authors": [
        {
            "name": "BlackCup",
            "email": "info@blackcup.nl",
            "homepage": "https://www.blackcup.nl"
        }
    ],
    "homepage": "https://github.com/blackcup/invites",
    "keywords": ["Laravel", "Invites"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BlackCup\\Invites\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlackCup\\Invites\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackCup\\Invites\\InvitesServiceProvider"
            ],
            "aliases": {
                "Invites": "BlackCup\\Invites\\Facades\\Invites"
            }
        }
    }
}
